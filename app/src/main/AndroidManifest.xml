        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.HelloActivity"
            android:label="@string/app_name"
            android:theme="@style/ActivityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.GuideActivity"/>
        <activity android:name=".activity.ChangeActivity"/>
        <activity android:name=".activity.RegisterActivity"/>
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".activity.TiXianActivity"/>
        <activity android:name=".activity.ZhangDanActivity"/>
        <activity android:name=".activity.ZhongXinActivity"/>
        <activity
            android:name=".activity.ChongZhiActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".activity.ErWeiMaActivity"/>
        <activity android:name=".activity.FeedBackActivity"/>
        <activity android:name=".activity.InfoActivity"/>
        <activity android:name=".activity.RenZhengActivity"/>

        <activity android:name=".activity.WalletActivity"/>
        <activity android:name=".activity.CzDetailActivity"/>
        <activity android:name=".activity.TxDetailActivity"/>
        <activity
            android:name=".activity.StockDetailActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".activity.CashActivity"/>
        <activity android:name=".activity.HuaBiActivity"/>
        <activity android:name=".activity.SilverActivity"/>
        <activity android:name=".activity.SilverMallActivity"/>
        <activity android:name=".activity.MallForActivity"/>
        <activity android:name=".activity.ContactActivity"/>
        <activity android:name=".activity.TeamDetailActivity"/>
        <activity android:name=".activity.InvestedTeamActivity"/>
        <activity android:name=".activity.SecurityActivity"/>
        <activity android:name=".activity.TeamCrewActivity"/>
        <activity android:name=".activity.MyTeamActivity"/>
        <activity android:name=".activity.NoticeActivity"/>
        <activity android:name=".activity.NoticeDetailActivity"/>

        <activity
            android:name=".activity.OrderDialogActivity"
            android:theme="@style/DialogTransparent"/>

        <activity android:name=".activity.CashDeclareActivity"/>
        <activity android:name=".activity.NewHandActivity"/>
        <activity android:name=".activity.InvestDetailActivity"/>

        <activity android:name=".activity.ChartDetailActivity"
                  android:screenOrientation="landscape"/>

        <activity android:name=".activity.GestureActivity"/>
        <activity android:name=".activity.GestureTipActivity"/>
        <activity android:name=".activity.DeblockActivity"/>
    </application>
</manifest>
